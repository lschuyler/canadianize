{
  "name": "lschuyler/canadianize",
  "description": "Canadianize WordPress plugin.",
  "minimum-stability": "stable",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Lisa Schuyler",
      "email": "lverkley@gmail.com",
      "homepage": "https://lisa.blog",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "composer/installers": "^1"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.1 || ^0.5 || ^0.6.2 || ^0.7",
    "phpunit/phpunit": "^8.5",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.5.2",
    "wp-coding-standards/wpcs": "^2",
    "yoast/wp-test-utils": "^1"
  },
  "support": {
    "issues": "https://github.com/lschuyler/canadianize/issues",
    "source": "https://github.com/lschuyler/canadianize/"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
